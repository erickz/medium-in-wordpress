{
  "name": "erickz/medium-in-wordpress",
  "type": "project",
  "description": "Integrates the medium api into wordpress",
  "keywords": [
    "medium",
    "wordpress"
  ],
  "require": {
    "guzzlehttp/guzzle": "6.*"
  },
  "autoload": {
    "psr-4": {
      "MediumInWp\\Switches\\": "src/switches",
      "MediumInWp\\Registers\\": "src/registers"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}